# Server Configuration
NODE_ENV=development
PORT=3000
API_VERSION=v1

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=cms_pa_system
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL=false

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# RabbitMQ Configuration
RABBITMQ_URL=amqp://localhost:5672
RABBITMQ_EXCHANGE=cms_pa_events
RABBITMQ_QUEUE_PREFIX=cms_pa

# Security Configuration
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRATION=24h
ENCRYPTION_KEY=your-encryption-key-32-chars-min
TLS_MIN_VERSION=TLSv1.3

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# External Services
FHIR_BASE_URL=https://api.example.com/fhir
X12_GATEWAY_URL=https://x12.example.com
ELIGIBILITY_API_URL=https://eligibility.example.com

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/cms-pa-system.log
